<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">
  <description>An  example that demonstrates simple IoC features.</description>
  <object name="UserInfoService" type="StudyOA.SpringNet.UserInfoService, StudyOA.SpringNet">
    <property name="UserName" value="dfdf"/>
    <property name="person" ref="Person"/>
  </object>
  <object name="Person" type="StudyOA.SpringNet.Person, StudyOA.SpringNet">
    <property name="Age" value="18"/>
  </object>
</objects>